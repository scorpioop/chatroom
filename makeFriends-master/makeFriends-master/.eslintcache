=>{\n    this.setState({\n      icon\n    })\n    this.props.setHandleHeader(text)\n\n  }\n}","D:\\聊天室\\makeFriends-master\\makeFriends-master\\src\\api\\ajax.js",["127"],"import axios from 'axios'\n\nexport default function ajax (url, data = {}, types = 'GET') {\n  if (types === 'GET') {\n    // 原始数据是：{username：Tom，password：123}\n    // 格式化的数据：username=Tom&password=123\n    if (Object.keys(data).length === 0) {\n      return axios.get(url)\n    } else {\n      let paramStr = ''\n      Object.key(data).forEach(key => {\n        paramStr += key + '=' + data[key] + '&'\n      })\n      if (paramStr != '') {\n        paramStr = paramStr.substring(0, paramStr.lastIndexOf('&'))\n        url = url + '?' + paramStr\n      }\n      return axios.get(url)\n    }\n  } else {\n    return axios.post(url, data)\n  }\n}\n",{"ruleId":"128","replacedBy":"129"},{"ruleId":"130","replacedBy":"131"},{"ruleId":"132","severity":1,"message":"133","line":2,"column":8,"nodeType":"134","messageId":"135","endLine":2,"endColumn":10},{"ruleId":"136","severity":1,"message":"137","line":90,"column":16,"nodeType":"138","messageId":"139","endLine":90,"endColumn":18},{"ruleId":"136","severity":1,"message":"137","line":98,"column":34,"nodeType":"138","messageId":"139","endLine":98,"endColumn":36},{"ruleId":"136","severity":1,"message":"137","line":24,"column":23,"nodeType":"138","messageId":"139","endLine":24,"endColumn":25},{"ruleId":"136","severity":1,"message":"137","line":23,"column":25,"nodeType":"138","messageId":"139","endLine":23,"endColumn":27},{"ruleId":"140","severity":1,"message":"141","line":81,"column":17,"nodeType":"142","endLine":88,"endColumn":19},{"ruleId":"132","severity":1,"message":"143","line":1,"column":17,"nodeType":"134","messageId":"135","endLine":1,"endColumn":26},{"ruleId":"140","severity":1,"message":"141","line":29,"column":15,"nodeType":"142","endLine":32,"endColumn":17},{"ruleId":"140","severity":1,"message":"141","line":35,"column":15,"nodeType":"142","endLine":38,"end